{
  "name": "rttm-syntax-hl",
  "displayName": "RTTM Syntax HL",
  "description": "Universal RTTM syntax highlighting support for speaker diarization, ASR, and audio annotation tasks.",
  "version": "0.2.0",
  "publisher": "z3r-dev",
  "homepage": "https://github.com/mxpanf/rttm-syntax-vscode",
  "author": {
    "name": "mxpanf"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#292d3e",
    "theme": "dark"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "rttm",
    "syntax",
    "grammar",
    "highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mxpanf/rttm-syntax-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/mxpanf/rttm-syntax-vscode/issues"
  },
  "editor.snippetSuggestions": "inline",
  "editor.quickSuggestions": {
    "other": true
  },
  "contributes": {
    "iconThemes": [
      {
        "id": "rttm-file-icon",
        "label": "RTTM File Icon Theme",
        "path": "./icon-theme.json"
      }
    ],
    "languages": [
      {
        "id": "rttm",
        "aliases": [
          "RTTM",
          "rttm",
          "Rich Transcription Time Marked"
        ],
        "extensions": [
          ".rttm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rttm",
        "scopeName": "source.rttm",
        "path": "./syntaxes/rttm.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "rttm",
        "path": "./snippets/rttm.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.80.0"
  }
}
